groups:
- name: RabbitMQ Alerts
  rules:
  - alert: Messages are not consumed
    expr: rabbitmq_queue_messages_ready_total > 0
    for: 1m
    labels:
      severity: warning
    annotations:
      summary: "{{ $labels.job }} Region Messages are not consumed [ready_msgs={{ $value }}]"
      description: "{{ $labels.job }} Region Messages are not consumed for 3 minutes."
